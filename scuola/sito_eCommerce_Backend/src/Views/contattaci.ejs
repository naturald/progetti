            <textarea id="message_contactUs" cols="30" rows="10" placeholder="Messagio" autocomplete="off"></textarea>
            <input type="submit" value="Invia" id="submit_contactUs">
        </form>
    </div>

    <%- include("partials/footer"); %> 
    
